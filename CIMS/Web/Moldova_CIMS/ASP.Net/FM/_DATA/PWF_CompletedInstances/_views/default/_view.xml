<view select="true" preview="false">
  <header title="" />
  <description title="" />
  <row keyfield="InstanceId">
    <cell name="WorkflowName" title="Workflow Name" />
    <cell name="LastStateTitle" title="Final State" width="150" />
    <cell name="StartedOn" title="Started On" width="100" />
    <cell name="CompletedOn" title="Completed On" width="100" />
    <cell name="Version" title="Ver" width="50" align="center" />
