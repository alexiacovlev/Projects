<view iconurl="ico.png" icon="true" actions="true">
  <row keyfield="Name">
    <cell name="Name" title="Name" width="200" />
    <cell name="Title" title="Title" />
    <cell name="Description" title="Description" width="100" />
    <cell name="LastAuthor" title="Last author" width="100" />
    <cell name="ModifiedOn" title="Modified On" width="100" />
  </row>
  <fetch>
    <sort>
      <column name="Name" ascending="true" />
    </sort>
    <filter />
  </fetch>
  <rowactions>
    <item title="Export to PDF" handler="URL:ASP.Net/Resources/FM/DataViewer/Print/DocumentReport.aspx" window="NEW" attributes="t=MY_SmartUsers,template=[id],id=81,type=PDF" width="200" height="150" scroll="false" full="false" image="PDF.png" />
    <item title="Export to WORD" handler="URL:ASP.Net/Resources/FM/DataViewer/Print/DocumentReport.aspx" image="Word.png" window="NEW" attributes="t=MY_SmartUsers,template=[id],id=81,type=DOCX" width="200" height="150" scroll="false" full="false" />
  </rowactions>
</view>