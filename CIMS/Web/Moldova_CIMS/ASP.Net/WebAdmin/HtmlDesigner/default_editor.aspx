        <script type="text/javascript">
                var pageID = "<%= pageID %>";
                var lcid = "<%= lcid %>";
                var bClose = false;
                var bChanged = false;
                var mode = "HTML";
                var $tbSource, $panelDesign;

                function GetRadWindow() {
                        var oWindow = null;
                        if (window.radWindow) oWindow = window.radWindow;
                        else if (window.frameElement.radWindow) oWindow = window.frameElement.radWindow;
                        return oWindow;
                }

                function setContent(content) {
                        var editor = $find("editor1");
                        if (editor) editor.set_html(content);  //set content from the parent page to RadEditor in RadWindow
                }


                function OnClientCommandExecuting(editor, args) {
                        var commandName = args.get_commandName();   //returns the executed command

                        if (commandName == "SaveAndClose") {
													bClose= true;
													args.set_cancel(true); //cancel the SaveAndClose command 
                          SaveHtml(editor.get_html(true));
                        }
                }


                function LoadHtml() {
                        $.get("LoadHtml.aspx?id=" + pageID + "&lcid=" + lcid
                                , null
                                , setContent
                                , "html");
                }
					
                function SaveHtml(html) {
                        html= _HtmlEncode(html);

                        //$("#btnSave").attr('disabled', 'disabled');
                        $.post("SaveHtml.aspx?id=" + pageID + "&lcid=" + lcid
                                                        , html
                                                        , function (result) {
																																
                                                                //$("#btnSave").removeAttr('disabled');
                                                                if (result != "") {
                                                                        alert(result);
                                                                } else if (!bClose) {
                                                                        bChanged = false;
                                                                        //$("#lblInfo").show().fadeOut(2000);
                                                                } else {
                                                                        Close();
                                                                }
                                                                bClose = false;
                                                        }
                                                        , "html"
                        );
                }
                function _HtmlEncode(s) {
                        return s.replace(/&/g, "&amp;").replace(/</g, "&lt;").replace(/>/g, "&gt;");
                }
								function Close() {
                        window.parent.location.reload();
                }

                function OnClientLoad(editor) {
                        //editor.fire("ToggleScreenMode"); //set RadEditor in Full Scree mode
                        LoadHtml();
                }

        </script>

