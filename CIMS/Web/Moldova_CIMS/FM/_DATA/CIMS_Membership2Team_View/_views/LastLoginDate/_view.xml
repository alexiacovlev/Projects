<view select="true" preview="true" icon="false">
  <header title="" />
  <description title="" />
  <row keyfield="UserId">
    <cell name="FullName" title="FullName" />
    <cell name="LastLoginDate" title="LastLoginDate" width="100" />
    <cell name="Email" title="Email" width="100" />
  </row>
  <hidden />
  <fetch>
    <sort>
      <column name="LastLoginDate" ascending="false" />
    </sort>
    <filter>
      <condition attribute="ApplicationId" operator="eq-appid" />
    </filter>
  </fetch>
</view>