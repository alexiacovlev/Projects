<form labelwidth="160">
  <header title="Applicant" />
  <description title="" />
  <tabs>
    <tab title="General" name="tab_general">
      <sections>
        <section title="Info" name="sec_info" showlabel="false" decoration="line" columns="3">
          <rows>
            <row>
              <cell title="Admission" name="Admission" colspan="2">
                <properties requiredlevel="required" readonly="true" />
                <event>var data = frm.getLookupInfo("Admission","TeamId,Year");
frm.setValue("TeamId",data["TeamId"]);</event>
              </cell>
              <cell title="Person Status" name="PersonStatus">
                <properties readonly="true" />
              </cell>
            </row>
            <row>
              <cell title="College" name="TeamId" colspan="2">
                <properties readonly="true" requiredlevel="required" />
              </cell>
              <cell />
            </row>
            <row>
              <cell title="Specialty" name="Specialty" colspan="2">
                <properties requiredlevel="required" />
              </cell>
              <cell />
            </row>
          </rows>
        </section>
        <section title="Personal Data" name="sec1257" showlabel="true" decoration="line" columns="3">
          <rows>
            <row>
              <cell title="First Name" name="FirstName">
                <properties requiredlevel="required" />
              </cell>
              <cell title="Middle Name" name="MiddleName">
                <properties requiredlevel="recommended" />
              </cell>
              <cell title="Last Name" name="LastName">
                <properties requiredlevel="required" />
              </cell>
            </row>
            <row>
              <cell title="Birthday" name="BirthDay">
                <properties requiredlevel="required" />
              </cell>
              <cell title="Gender" name="Gender">
                <properties requiredlevel="required" />
              </cell>
              <cell title="Living Environment" name="LivingEnvironment" />
            </row>
            <row>
              <cell title="Place Of Birth" name="BirthPlace" colspan="3">
                <properties requiredlevel="required" />
              </cell>
            </row>
          </rows>
        </section>
        <section title="Identification Document" name="sec1125" showlabel="true" decoration="line" columns="3">
          <rows>
            <row>
              <cell title="Document Type" name="ID_Type" />
              <cell title="Number" name="ID_Number" />
              <cell title="Issued On" name="ID_IssuedOn" />
            </row>
            <row>
              <cell title="Issued By" name="ID_IssuedBy" colspan="2" />
              <cell />
            </row>
            <row>
              <cell title="IDNP" name="IDNP" />
              <cell title="Citizenship" name="Citizenship" />
              <cell title="Ethnicity" name="Ethnicity" />
            </row>
          </rows>
        </section>
        <section title="Military Evidence" name="sec1393" showlabel="true" decoration="line" columns="3">
          <rows>
            <row>
              <cell title="Type Of Evidence" name="MilitaryTypeOfEvidence" />
              <cell title="Document Nr" name="MilitaryDocumentNr" />
              <cell title="Issued On" name="MilitaryDocumentIssuedOn" />
            </row>
          </rows>
        </section>
        <section title="Residence" name="sec143" showlabel="true" decoration="line" columns="3">
          <rows>
            <row>
              <cell title="Country" name="ResidenceCountry" />
              <cell title="Raion" name="ResidenceRaion" />
              <cell title="Locality" name="ResidenceLocality" />
            </row>
            <row>
              <cell title="Address" name="ResidenceAddress" colspan="2" />
              <cell title="EMail" name="ResidenceStudentEMail" />
            </row>
            <row>
              <cell title="Home Phone" name="ResidenceHomePhone" />
              <cell title="Parent Phone" name="ResidenceParentPhone" />
              <cell title="Mobile" name="ResidenceStudentMobile" />
            </row>
          </rows>
        </section>
      </sections>
    </tab>
    <tab title="Admission Details" name="tab2">
      <sections>
        <section title="Parents" name="sec2271" showlabel="true" decoration="line" columns="2">
          <rows>
            <row>
              <cell title="Father Name" name="FatherFullName" />
              <cell title="Full Name" name="MotherFullName" />
            </row>
          </rows>
        </section>
        <section title="Family Components" name="sec2326" showlabel="true" decoration="line" columns="2">
          <rows>
            <row>
              <cell title="Number Of Parents" name="NumberOfParents" />
              <cell title="Number Of Children In Family" name="NumberOfChildrenInFamily" />
            </row>
          </rows>
        </section>
        <section title="Admission Category" name="sec2522" showlabel="true" decoration="line" columns="2">
          <rows>
            <row>
              <cell title="Share Of Admission Vacancies" name="ShareOfAdmissionVacancies" />
              <cell />
            </row>
            <row>
              <cell title="Special Category" name="SpecialCategory" />
              <cell />
            </row>
            <row>
              <cell title="Language Of Studying" name="LanguageOfStudying" />
              <cell />
            </row>
          </rows>
        </section>
        <section title="Options" name="sec2697" showlabel="true" decoration="line" columns="2">
          <rows>
            <row>
              <cell title="Need A Dorm" name="NeedADorm" />
              <cell />
            </row>
            <row>
              <cell title="Only State Budget" name="OnlyStateBudget" />
              <cell />
            </row>
            <row>
              <cell title="Foreign Language Studied" name="ForeignLanguageStudied" />
              <cell />
            </row>
            <row>
              <cell title="Work Experience" name="WorkExperience" />
              <cell />
            </row>
            <row>
              <cell title="Olympic Awards" name="OlympicAwards" />
              <cell />
            </row>
            <row>
              <cell title="Olimpic Awards Details" name="OlimpicAwardsDetails" colspan="2" />
            </row>
          </rows>
        </section>
      </sections>
    </tab>
    <tab title="Graduated From" name="tab3">
      <sections>
        <section title="Institution Details" name="sec3196" showlabel="true" decoration="line" columns="3">
          <rows>
            <row>
              <cell title="Institution Name" name="GraduatedInstitutionName" colspan="2" />
              <cell />
            </row>
            <row>
              <cell title="Country" name="GraduatedInstitutionCountry" />
              <cell title="Raion" name="GraduatedInstitutionRaion" />
              <cell title="Locality" name="GraduatedInstitutionLocality" />
            </row>
            <row>
              <cell title="Environment" name="GraduatedInstitutionEnvironment" />
              <cell />
              <cell />
            </row>
            <row>
              <cell title="Institution Type" name="GraduatedInstitutionType" />
              <cell title="Grade" name="GraduatedInstitutionGrade" />
              <cell title="Year" name="GraduatedInstitutionYear">
                <properties behavior="1900|2020" />
              </cell>
            </row>
          </rows>
        </section>
        <section title="Certificate Of Education" name="sec3413" showlabel="true" decoration="line" columns="3">
          <rows>
            <row>
              <cell title="Type" name="CertificateOfEducationType" />
              <cell title="Number" name="CertificateOfEducationNumber" />
              <cell title="Issued On" name="CertificateOfEducationIssuedOn" />
            </row>
          </rows>
        </section>
        <section title="Marks" name="sec3325" showlabel="true" decoration="line" columns="2">
          <rows>
            <row>
              <cell title="Average Mark" name="GraduatedInstitutionAverageMark" />
              <cell />
            </row>
          </rows>
        </section>
      </sections>
    </tab>
    <tab title="Attachments" name="tab5">
      <sections>
        <section title="Attachments" name="sec5947" showlabel="false" decoration="line" columns="2">
          <rows>
            <row>
              <cell name="control_939" colspan="2">
                <content>&lt;control type="GRID" height="400"&gt;
 &lt;set name="TableName" value="CIMS_Attachments" /&gt;
 &lt;set name="ViewName" value="default" /&gt;
 &lt;set name="OpenForm" value="FORM:default" /&gt;
 &lt;set name="CreateForm" value="FORM:default" /&gt;
 &lt;set name="AllowCreate" value="true" /&gt;
 &lt;set name="AllowDelete" value="true" /&gt;
 &lt;set name="HandlerParameters" value="800,500" /&gt;
 &lt;set name="ShowGridFilterPanel" value="false" /&gt;
 &lt;set name="ShowActionsMenu" value="true" /&gt;
 &lt;set name="InputFilterField" value="ParentId=[KeyValue]" /&gt;
&lt;/control&gt;</content>
              </cell>
            </row>
          </rows>
        </section>
      </sections>
    </tab>
  </tabs>
  <hidden>
    <cell title="Created By" name="CreatedBy">
      <properties oncreatevalue="~USERID" />
    </cell>
    <cell title="Created IP" name="CreatedIP">
      <properties oncreatevalue="~IP" />
    </cell>
    <cell title="Created On" name="CreatedOn">
      <properties oncreatevalue="~NOW" />
    </cell>
    <cell title="Modified By" name="ModifiedBy">
      <properties oncreatevalue="~USERID" onupdatevalue="~USERID" />
    </cell>
    <cell title="Modified IP" name="ModifiedIP">
      <properties oncreatevalue="~IP" onupdatevalue="~IP" />
    </cell>
    <cell title="Modified On" name="ModifiedOn">
      <properties oncreatevalue="~NOW" onupdatevalue="~NOW" />
    </cell>
    <cell title="Application" name="ApplicationId">
      <properties oncreatevalue="~APPLICATIONID" />
    </cell>
  </hidden>
  <windowMask>Applicant: #FirstName# #LastName#</windowMask>
</form>