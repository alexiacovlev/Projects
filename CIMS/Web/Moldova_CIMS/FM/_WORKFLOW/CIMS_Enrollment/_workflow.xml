  <Sequence>
    <Switch>
      <Case test="INPUT:id = ''" details=";">
        <Sequence>
          <Invoke name="FORM1" title="" control="Form">
            <Settings table="CIMS_DUMMY" form="admission1" provider="DUMMY" />
